{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node",
        "config": { "NODE_ENV": "production" }
      },
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": { "NODE_ENV": "production" }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "src/index.js"
      }
    ]
  }
  